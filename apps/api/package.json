    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.3",
    "@nestjs/core": "^10.4.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-fastify": "^10.4.3",
    "@prisma/client": "^5.19.1",
    "@fastify/cors": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.28.1",
    "prisma": "^5.19.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2",
    "@types/node": "^20",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
